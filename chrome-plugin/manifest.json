{
  "manifest_version": 3,
  "name": "Agent Button Extension",
  "version": "1.0",
  "description": "Adds an agent button to specified pages",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
} 